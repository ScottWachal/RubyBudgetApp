<h1>Listing accounts</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Balance</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @accounts.each do |account| %>
  <tr>
    <td><%= account.name %></td>
    <td><%= account.balance %></td>
    <td><%= link_to 'Show', profile_account_path(@profile_account) %></td>
    <td><%= link_to 'Edit', edit_profile_account_account_path(@profile_account, account) %></td>
  </tr>
<% end %>
</table>

