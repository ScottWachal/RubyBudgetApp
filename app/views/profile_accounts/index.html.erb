<h1>Listing profile_accounts</h1>

<table class="table">
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
  </tr>

<% @profile_accounts.each do |profile_account| %>
  <tr>
    <td><%= link_to profile_account.name, profile_account %></td>
    <td><%= profile_account.description %></td>
    <td><%= link_to 'Edit', edit_profile_account_path(profile_account) %></td>
    <td><%= link_to 'Destroy', profile_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Profile account', new_profile_account_path %>
